<Window x:Class="ReactiveUiBindCommandSamples.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReactiveUiBindCommandSamples.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="TextBox1">Focus me</TextBox>
            <Button x:Name="ButtonClickMe">And then click me</Button>
            <TextBox x:Name="TextBox2" GotFocus="TextBox2_OnGotFocus" LostFocus="TextBox2_OnLostFocus"></TextBox>        
        </StackPanel>
    </Grid>
</Window>
